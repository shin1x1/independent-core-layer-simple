{
  "name": "cakephp2/app",
  "require": {
	"php": ">=5.6.0",
	"cakephp/cakephp": "2.10.*",
	"cakephp/orm": "3.6.*",
	"josegonzalez/dotenv": "3.*"
  },
  "require-dev": {
	"phpunit/phpunit": "3.7.*"
  },
  "autoload": {
	"psr-4": {
	  "App\\": "../cakephp3/src/",
	  "Acme\\": "../core/Acme/"
	}
  },
  "suggest": {
	"cakephp/cakephp-codesniffer": "Easily check code formatting against the CakePHP coding standards."
  },
  "bin": [
	"app/Console/cake"
  ],
  "config": {
	"vendor-dir": "app/Vendor/"
  }
}
